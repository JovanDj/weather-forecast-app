{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": ["/favicon.ico", "/index.html", "/*.css", "/*.js"]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": ["/assets/**", "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api-performance",
      "urls": ["/search.json"],
      "cacheConfig": {
        "maxAge": "1d",
        "strategy": "performance"
      }
    },
    {
      "name": "api-performance",
      "urls": ["/current.json"],
      "cacheConfig": {
        "maxAge": "15m",
        "strategy": "performance"
      }
    }
  ]
}
